<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuUsuario">

    <Button
        android:id="@+id/btn_MisMetapps"
        android:layout_width="145dp"
        android:layout_height="95dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="56dp"
        android:onClick="OnClick"
        android:text="Mis Metapps"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/bnt_CrearMetapp"
        android:layout_width="145dp"
        android:layout_height="95dp"
        android:layout_marginTop="52dp"
        android:onClick="OnClick"
        android:text="Nueva Metapp"
        app:layout_constraintStart_toStartOf="@+id/btn_MisMetapps"
        app:layout_constraintTop_toBottomOf="@+id/btn_MisMetapps" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="151dp"
        android:layout_height="98dp"
        android:layout_marginTop="64dp"
        android:text="Bienvenido Usuario"
        app:layout_constraintStart_toStartOf="@+id/btn_MisMetapps"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>